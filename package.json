{
  "name": "media-recorder",
  "version": "1.0.1",
  "description": "Media Recorder",
  "main": "./dist/index.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "debug": false,
                "targets": {
                  "browsers": [
                    "Explorer 11",
                    "last 1 Chrome versions",
                    "last 1 FireFox versions",
                    "last 1 Safari versions",
                    "last 1 Edge versions",
                    "ios 8"
                  ]
                },
                "useBuiltIns": "usage",
                "corejs": "3"
              }
            ]
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reallyenglish-global/media-recorder"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "jquery": "^3.4.1",
    "regenerator-runtime": "^0.13.3",
    "underscore": "^1.9.2",
    "webrtc-adapter": "4.0.2"
  },
  "devDependencies": {
    "rels-gulp": "git+ssh://git@github.com/reallyenglish-global/rels-gulp#v5.2.5"
  },
  "scripts": {
    "build": "./node_modules/.bin/babel ./lib --out-dir dist",
    "postinstall": "npm run build",
    "test": "gulp test",
    "format": "prettier -c \"lib/**/*{.js*,.scss}\"",
    "lint": "eslint lib"
  },
  "engine-strict": true,
  "engines": {
    "node": "^12",
    "npm": "^6"
  }
}
